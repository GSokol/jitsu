{
  "name": "cra-en",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@jitsu/sdk-js": "^2.2.2",
    "@types/react-grid-layout": "^1.1.2",
    "antd": "^4.13.1",
    "axios": "^0.21.4",
    "classnames": "^2.2.6",
    "firebase": "^9.4.1",
    "highlight.js": "^11.3.1",
    "hotkeys-js": "^3.8.7",
    "leader-line-new": "^1.1.9",
    "lodash": "^4.17.21",
    "logrocket": "^1.0.14",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "monaco-editor": "^0.28.1",
    "murmurhash": "^2.0.0",
    "posthog-js": "^1.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-monaco-editor": "^0.46.0",
    "react-reflex": "^4.0.3",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.4",
    "recharts": "^2.0.8",
    "uuid": "^8.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(react-syntax-highlighter)",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  },
  "scripts": {
    "clean": "rm -rf ./node_modules/ build dist",
    "start": "NODE_ENV=development craco start",
    "build": "NODE_ENV=production craco build",
    "stats": "rm -rf build && yarn build -- --stats",
    "bundle": "npx webpack-bundle-analyzer ./build/bundle-stats.json",
    "analyze": "source-map-explorer 'build/static/js/*.js' --no-border-checks",
    "test": "craco test --watchAll",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache",
    "prettier:check": "prettier --check --config ./.prettierrc.json --ignore-path ./.prettierignore .",
    "prettier:fix": "prettier --write --config ./.prettierrc.json --ignore-path ./.prettierignore .",
    "eslint:check": "eslint . --ext .tx,.tsx --ignore-pattern node_modules/",
    "eslint:fix": "eslint . --ext .tx,.tsx --ignore-pattern node_modules/ --fix",
    "all:check": "yarn run prettier:check && yarn run eslint:check"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "^4.5.0",
    "@craco/craco": "^6.1.1",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.11",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.168",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^9",
    "babel-plugin-lodash": "^3.3.4",
    "craco-antd": "^1.19.0",
    "craco-less": "^1.17.1",
    "monaco-editor-webpack-plugin": "^4.2.0",
    "msw": "^0.35.0",
    "prettier": "^2.4.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-scripts": "^4.0.3",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.4.3",
    "web-vitals": "^1.0.1",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "resolutions": {
    "immer": "^9.0.6",
    "path-parse": "^1.0.7",
    "ansi-regex": "^5.0.1",
    "tmpl": "^1.0.5",
    "set-value": "^4.1.0",
    "glob-parent": "^6.0.2",
    "postcss": "^7.0.39",
    "prismjs": "^1.25.0",
    "dns-packet": "^5.3.0",
    "ws": "^8.2.3",
    "tar": "^6.1.11",
    "nth-check": "^2.0.1",
    "browserslist": "^4.17.3"
  }
}
